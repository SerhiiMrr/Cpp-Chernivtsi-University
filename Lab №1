#include <iostream>
#include <math.h>
#include <conio.h>
#include <stdlib.h>
using namespace std;

int main()
{
    system("chcp 1251>null");
    int y,x,c,a;
    cout << "Вкажіть y,x,c та a:";
    cin >>y>>x>>c>>a;
    double result = (pow(a,5)+acos(a+pow(x,3))-pow(sin(y-c),4))/(pow(sin(x+y),3)+abs(x-y));
    cout <<"Результат:" << result;
    _getch();
    return 0;
}
